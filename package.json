{
  "name": "react-package-template",
  "description": "A template for creating React packages",
  "keywords": [
    "react",
    "package",
    "template",
    "typescript"
  ],
  "author": "Andrew Dyer (https://github.com/andrewdyer)",
  "version": "1.0.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/andrewdyer/react-package-template.git"
  },
  "main": "./dist/index.cjs.js",
  "types": "./dist/index.d.ts",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.es.js",
      "require": "./dist/index.cjs.js",
      "default": "./dist/index.umd.js"
    }
  },
  "scripts": {
    "build": "npm run clean && tsc -b && vite build",
    "clean": "rimraf dist",
    "lint": "eslint .",
    "format": "prettier --write .",
    "prepare": "husky",
    "prepublishOnly": "npm run build",
    "storybook": "storybook dev -p 6006",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage --no-cache --runInBand",
    "type-check": "tsc --noEmit",
    "yalc:publish": "yalc publish",
    "yalc:push": "yalc push",
    "build-storybook": "storybook build"
  },
  "devDependencies": {
    "@eslint/js": "^9.33.0",
    "@storybook/addon-docs": "^9.1.3",
    "@storybook/react-vite": "^9.1.3",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.0.1",
    "@types/jest": "^29.5.14",
    "@types/react": "^19.1.10",
    "@types/react-dom": "^19.1.7",
    "@vitejs/plugin-react": "^5.0.0",
    "eslint": "^9.33.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "eslint-plugin-storybook": "^9.1.3",
    "globals": "^16.3.0",
    "husky": "^9.1.7",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "prettier": "^3.3.3",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "rimraf": "^6.0.1",
    "storybook": "^9.1.3",
    "ts-jest": "^29.2.5",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.39.1",
    "vite": "^7.1.2",
    "vite-plugin-dts": "^4.5.4"
  },
  "peerDependencies": {
    "react": "^19.1.1",
    "react-dom": "^19.1.1"
  },
  "sideEffects": false,
  "files": [
    "dist"
  ]
}
